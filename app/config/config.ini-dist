[config]

; Path to Git checkout of Typolib, it will contain setup/update scripts
; and the web folder
install=${TYPOLIBDIR}/

; Path to configuration files for Typolib
config=${TYPOLIBDIR}/app/config

; Typolibâ€™ Github account
typolib_github_account=TheoChevalier

; Client Github username
